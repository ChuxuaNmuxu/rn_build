{
  "name": "cjhms_rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint --ext .js ./src --fix",
    "svg-to-xml": "cd src/scripts && node ./svgToXml.js",
    "iconfontMap": "cd ./src/scripts && pip install fonttools && python iconfontMap.py ../public/icon/iconfont.ttf iconfont.js",
    "mv-iconfontMap": "npm run iconfontMap && cd ./src/scripts && mv ./iconfont.js ../public/icon/ && cd ../public/icon && mv ./iconfont.ttf ../../../android/app/src/main/assets/fonts/",
    "move-iconfontMap": "npm run iconfontMap && cd ./src/scripts && move ./iconfont.js ../public/icon/ && cd ../public/icon && move ./iconfont.ttf ../../../android/app/src/main/assets/fonts/",
    "dev-menu": "adb shell input keyevent 82",
    "devtools": "react-devtools",
    "dev": "react-native run-android",
    "dev-android": "react-native run-android",
    "dev-ios": "react-native run-ios",
    "build-js": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build": "npm run build-js && cd android && gradlew.bat assembleRelease --console plain",
    "build-1": "cd android && gradlew assembleRelease",
    "test-build&&install": "cd android && gradlew installRelease",
    "install-dev": "adb install ./android/app/build/outputs/apk/app-debug.apk",
    "install-build": "adb install ./android/app/build/outputs/apk/app-release.apk",
    "clear-build-cache": "cd android && gradlew clean",
    "clear-build-file": "rimraf android/app/build",
    "clear": "npm run clear-build-cache && npm run clear-build-cache",
    "clear-all": "yarn cache clean -f && npm cache clean -f && cnpm cache clean -f && npm run clear",
    "gradle-debug": "cd android && gradlew compiledebug --debug",
    "gradle-info": "cd android && gradlew compiledebug --info",
    "gradle-stacktrace": "cd android && gradlew compiledebug --stacktrace",
    "stop": "adb shell am force-stop com.cjhms_rn",
    "start-app": "adb shell am start -n com.cjhms_rn/.MainActivity",
    "remove": "adb uninstall com.cjhms_rn",
    "deploy": "code-push release-react 课业 android -t 1.0.3 --des 2018.11.05_05 -d Staging -m true",
    "deploy-history": "code-push deployment history 课业 Staging",
    "clear-deploy-history": "code-push clear history 课业 Staging"
  },
